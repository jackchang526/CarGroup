            window.open(document.getElementById('cheyisou').value);
        }
        else
        {
            window.open(document.getElementById('cheyisou').value + encodeURIComponent(document.getElementById('sug_txt_keyword').value) + '/');
        }
    }
    function addevent(elm, evType, fn) {
        if (elm.addEventListener) {
            elm.addEventListener(evType, fn, false); //DOM2.0
        }
        else if (elm.attachEvent) {
            elm.attachEvent('on' + evType, function (e) {
                fn.call(elm, e); //改变this指向
            });
        }
        else {
            elm["on" + evType] = fn;
        }
    }
   function keycode(e) {
        return e = e || window.event, e.which || e.keyCode || e.charCode;
    }
    addevent(document.getElementById('sug_txt_keyword'), "keydown", function (t) {
        var n = keycode(t);
        switch (n) {
            case 13: Search();
                break;
        }
    });
    addevent(document.getElementById('but_txt_keyword'), "click", function (t) {
        Search();
    });

    </script>
