        <br />
        <!-- 车身形式 end  -->
        <!-- 选车按钮 start  -->
        <div class="btn-warp-box">
            <a  id="clearSelected" class="ljt-btn ljt-current"></a>
            <a  id="selectResult" class="car-btn car-current">有0款车型符合条件</a>
        </div>
        @*<div class="btn-warp-box">
