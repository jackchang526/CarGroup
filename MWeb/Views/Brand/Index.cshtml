@using BitAuto.Utils;
@{
    Layout = null;
    string wtmcid = Request.QueryString["wtmcid"];
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style" />
    <meta content="telephone=no" name="format-detection" />
    <title>【@ViewData["_brandName"]】@ViewData["_brandName"]汽车报价_图片_@DateTime.Now.Year@ViewData["_brandName"]新款车型-手机易车网</title>
    <meta name="Keywords" content="@ViewData["_brandName"],@ViewData["_brandName"]汽车,@ViewData["_brandName"]汽车报价,@DateTime.Now.Year@ViewData["_brandName"]新款车型-手机易车网,car.m.yiche.com" />
    <meta name="Description" content="@ViewData["_brandName"]汽车:提供最新 @ViewData["_brandName"]汽车报价,@ViewData["_brandName"]汽车图片,@ViewData["_brandName"]汽车新闻,视频,口碑,问答,二手车等。@ViewData["_brandName"]在线询价、低价买车尽在手机易车网。" />
    <!--#include file="/ushtml/0000/myiche2015_cube_pinpai-1077.shtml"-->
    @if (wtmcid == "mbdofcl")
    {
        <script src="//msite.baidu.com/sdk/c.js?appid=1569705116260104"></script>
        <link rel="canonical" href="http://car.bitauto.com/@(ViewData["_brandSpell"])/" /> 
        <script type="application/ld+json">
            {
            "@@context": "https://zhanzhang.baidu.com/contexts/cambrian.jsonld",
            "@@id": "@(Request.Url.ToString())",
            "title": "【@ViewData["_brandName"]】@ViewData["_brandName"]汽车报价_图片_@DateTime.Now.Year@ViewData["_brandName"]新款车型-手机易车网",
            "images": ["http://image.bitautoimg.com/bt/car/default/images/logo/masterbrand/png/100/m_@(ViewData["_masterId"])_100.png"],
            "description":"",
            "pubDate": "2017-09-11T08:00:01"
            }
        </script>
    }
</head>
<body>
    @Html.Partial("~/html/TopDownloadLayer.cshtml")
    <div class="mbt-page">
        <!-- header start -->
        <div class="op-nav">
            <a id="gobackElm" href="javascript:;" class="btn-return">返回</a>

            <div class="tt-name">
                <a href="http://m.yiche.com/" class="yiche-logo">汽车</a><h1>@ViewData["_brandReplaceName"]</h1>
            </div>
            <!--#include file="/include/pd/2016/wap/00001/201607_wap_common_ejdht_Manual.shtml"-->
        </div>
        <div class="op-nav-mark" style="display: none;"></div>
        <!-- header end -->
        @*品牌列表 start*@
        @Html.Raw(ViewData["_brandListHtml"])
        @*品牌列表 end*@
    </div>
    <script type="text/javascript">
        var url = "http://car.m.yiche.com/@(ViewData["_masterSpell"])/";
        var CarCommonBSID = "@(ViewData["_masterId"])";
        var CarCommonCBID = "@(ViewData["_brandId"])";
    </script>
   @*--footer start--*@
    <div class="footer15">
        <!--搜索框-->
        @Html.Partial("~/html/footersearch.cshtml")
        @Html.Partial("~/html/footerV3.cshtml")
    </div>
    <div class="float-r-box">
        <!--#include file="/include/pd/2014/wapCommon/00001/201511_wap_common_ycfd_Manual.shtml"-->
    </div>
    @*--footer end--*@
</body>
</html>
