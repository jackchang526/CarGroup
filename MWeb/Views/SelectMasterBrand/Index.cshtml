    <script type="text/javascript" src="http://image.bitautoimg.com/mergejs,s=carchannel/wirelessjsv2/v2/iscroll.min.js,carchannel/wirelessjsv2/v2/underscore.min.js,carchannel/wirelessjsv2/v2/swiper-3.3.1.min.js,carchannel/wirelessjsv2/v2/model.min.js,carchannel/wirelessjsv2/v2/rightswipe.min.js,carchannel/wirelessjsv2/v2/note.min.js?v=201712111100"></script>
    <script type="text/javascript">
        var $body = $('body');

        $(function () {
            if (typeof (bitLoadScript) == "undefined")
                bitLoadScript = function (url, callback, charset) {
                    var s = document.createElement("script"); s.type = "text/javascript"; if (charset) s.charset = charset;
                    if (s.readyState) { s.onreadystatechange = function () { if (s.readyState == "loaded" || s.readyState == "complete") { s.onreadystatechange = null; if (callback) callback(); } }; }
                    else { s.onload = function () { if (callback) callback(); }; }
                    s.src = url; document.getElementsByTagName("head")[0].appendChild(s);
                };

            bitLoadScript('http://image.bitautoimg.com/stat/PageAreaStatistics.js', function () {
                PageAreaStatistics.init("281");
            }, 'utf-8');
            var $body = $('body');
            //笔记本滑动效果
            $body.trigger('note');

            var reTemp = "@ViewBag.re";
            if (reTemp.toString() != "true") {  //右侧子品牌效果
                $body.trigger('rightswipe3', { model_hide: true });
            }

            //图片加载
            bitLoadScript("http://image.bitautoimg.com/carchannel/wirelessjsv2/jquery.lazyload.min.js", function () {
                $("span.brand-logo>img").lazyload({ effect: "fadeIn", threshold: 50 });
            }, "utf-8");
        });
        function reLocation() {
            $body.trigger('note');
        }

        function adCallback(objId) {
            var parentNode = document.getElementById(objId).parentNode;
            $(parentNode).imgSucceed({
                end: function () {  $body.trigger('note'); }
            });
            var insNode = $("#" + objId);
            if (insNode.parents(".m-hot-brands").length > 0) {
                insNode.find('a').each(function (index, current) {
                    $(current).attr('data-action', 'car');
                    $(current).click(function () {
                        $body.trigger('openRight', this);
                    });
                })
            }
        }
    </script>
