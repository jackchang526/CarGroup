<div class="leftmask mark leftmask3" style="display: none;"></div>
<div class="leftPopup car-model car" data-back="leftmask3" style="display: none;" data-key="car">
    <div class="swipeLeft swipeLeft-sub">
        <div class="loading">
            <img src="http://image.bitautoimg.com/uimg/mbt2015/images/loading.gif" alt="" />
            <p>正在加载...</p>
        </div>
    </div>
</div>
<!--车型层 end-->
<!--loading模板 start -->
<div class="template-loading" style="display: none;">
    <div class="loading">
        <img src="http://image.bitautoimg.com/uimg/mbt2015/images/loading.gif" />
        <p>正在加载...</p>
    </div>
</div>
<!--loading模板 end -->
<!--车型模板 start-->
<script type="text/template" id="carTemplate">
    <div class="swipe-header">
        <div class="choose-car-name-close bybrand_list">
            <div class="brand-logo-none-border"><img /></div>
            <span class="brand-name"></span>
            <!-- <a href="#" class="choose-car-btn-close">关闭</a> -->
            @*<a class="brand-link" href="###">
                <h6>BMW品牌专区</h6>
                <p>悦享纯粹驾驶乐趣</p>
            </a>*@
        </div>
        <div class="clear"></div>
        <div class="first-tags">
            <ul>
                <li class="current"><a href="#"><span>在售</span></a></li>
                <li><a href="#"><span>停售</span></a></li>
            </ul>
        </div>
    </div>
    <div class="swipe-content">
        <div class="swiper-container">
            <div class="swiper-wrapper">
                <div class="swiper-slide">
                    <div class="swipeLeft-box">
                        <div class="tt-list absolute">
                            { for(var i = 0 ; i < list.length ; i++){ }
                            {if(list[i].SellCount > 0){}
                            <!-- 车款列表 start -->
                            {if(list[i].Child.length > 0){ }
                            <div class="tt-small">
                                <span>{= list[i].BrandName }</span>
                            </div>
                            {}}
                            <!-- 图文混排横向 start -->
                            <div class="pic-txt-h pic-txt-9060">
                                <ul>
                                    { for(var j = 0 ;j < list[i].Child.length; j++){ }
                                    {if(list[i].Child[j].CsSaleState == '在销' || list[i].Child[j].CsSaleState == '待销'){}
                                    <li {= list[i].Child[j].SerialId.toString() ==(api.car.currentid.toString()) ? 'class="current"' :''}>
                                        <a class="imgbox-2" href="/{= list[i].Child[j].Allspell}/" data-id="{= list[i].Child[j].SerialId}">
                                            <div class="img-box">
                                                <img src="{= list[i].Child[j].ImageUrl}" alt="" />
                                            </div>
                                            <div class="c-box">
                                                <h4>{= list[i].Child[j].SerialName }{if(list[i].Child[j].Market == "即将上市"){}<em class="jijiangshangshi">{=list[i].Child[j].Market}</em>{}else if(list[i].Child[j].Market != ""){}<em class="the-new">{=list[i].Child[j].Market}</em>{}}</h4>
                                                <p><strong>{= list[i].Child[j].Price}</strong></p>
                                            </div>
                                        </a>
                                    </li>
                                    {}}
                                    {sellCount++}
                                    {}}
                                </ul>
                            </div>
                            <!-- 图文混排横向 end -->
                            {}}
                            {}}
                            {if(sellCount == 0){}
                            <div class="txt-hint">
                                没有在售车系
                            </div>
                            {}}
                        </div>
                    </div>
                </div>
                <div class="swiper-slide">
                    <div class="swipeLeft-box">
                        <div class="tt-list absolute">
                            { for(var i = 0 ; i < list.length ; i++){ }
                            {if(list[i].SaleStateCount > 0){}
                            <!-- 车款列表 start -->
                            {if(list[i].Child.length > 0){}
                            <div class="tt-small">
                                <span>{= list[i].BrandName }</span>
                            </div>
                            {}}
                            <!-- 图文混排横向 start -->
                            <div class="pic-txt-h pic-txt-9060">
                                <ul>
                                    { for(var j = 0 ; j < list[i].Child.length ; j++){}
                                    {if(list[i].Child[j].CsSaleState == '停销'){}
                                    <li {= list[i].Child[j].SerialId.toString() ==(api.car.currentid.toString()) ? 'class="current"' :''}>
                                        <a class="imgbox-2" data-id="{=list[i].Child[j].SerialId}" href="/{= list[i].Child[j].Allspell}/">
                                            <div class="img-box">
                                                <img src="{=list[i].Child[j].ImageUrl}" />
                                            </div>
                                            <div class="c-box">
                                                <h4>{=list[i].Child[j].SerialName }</h4>
                                                <p><strong>{=list[i].Child[j].Price}</strong></p>
                                            </div>
                                        </a>
                                    </li>

                                    {}}
                                    {salesCount++}
                                    {}}
                                </ul>
                            </div>

                            <!-- 图文混排横向 end -->
                            {}}
                            {}}
                            {if(salesCount == 0){}
                            <div class="txt-hint">
                                没有停售车系
                            </div>
                            {}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
