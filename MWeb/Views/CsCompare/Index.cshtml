        var MaxCarCount = 30;
        @(Html.Raw(ViewBag.SelectJson))
        function initCarInfo(carId) {
            if (carId != "") {
                loadJS.push("http://api.car.bitauto.com/CarInfo/GetCarParameter.ashx?v=20171211&isParamPage=1&carids=" + carId, "utf-8", function () {
                    initPageForCompare();
                });
            }
            else {
                carCompareJson = [];
                initPageForCompare();
            }
        }
        var carIds = '@(ViewBag.CarIds)';
        var carArray = [];
        if (carIds.indexOf(",") != -1) {
            carArray = carIds.split(",");
        }else{carArray.push(carIds);}
        var topCarIds = carArray.length >= MaxCarCount ? carArray.slice(0, MaxCarCount).join(",") : topCarIds = carArray.join(",");
        initCarInfo(topCarIds);
        ConditionSelectCar.InitHtml();
    </script>

